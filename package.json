{
	"name": "palette-matcher",
	"displayName": "Palette Matcher",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:palette.replace",
		"onCommand:palette.convert"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "palette.replace",
				"title": "Palette Matcher: Replace selected color with one in palette"
			},
			{
				"command": "palette.convert",
				"title": "Palette Matcher: Convert color"
			}
		],
		"configuration": {
			"title": "Palette",
			"properties": {
				"paletteMatcher.palette": {
					"type": "array",
					"default": null,
					"description": "An array of hex codes that comprise your palette"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/lodash": "^4.14.135",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0"
	},
	"dependencies": {
		"@types/chroma-js": "^1.4.1",
		"chroma-js": "^2.0.4",
		"lodash": "^4.17.11"
	}
}
